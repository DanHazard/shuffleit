

<div><%= @team.name %></div>
<% @team.players.each do |player| %>
  <li><%= link_to player.name, player_path(player.id)%></li>
<% end %>

<p>Win: <%= @team.wins %></p>
<p>Losses: <%= @team.losses %></p>

<%= link_to "Back to Teams", teams_path %>
<br>
<button><%= link_to "Edit Team", edit_team_path  %></button>
<%= button_to 'Disband Team', team_path(@team), method: :delete %>
